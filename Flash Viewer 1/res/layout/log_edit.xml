<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<include layout="@layout/title_area" android:id="@+id/linearLayout4_ref"
		android:layout_width="match_parent" android:layout_height="wrap_content" />
	<View android:id="@+id/view1" android:layout_width="wrap_content"
		android:background="@color/orange" android:layout_height="@dimen/normalHeight"
		style="@style/PanelElement"></View>
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_height="wrap_content" android:layout_width="match_parent"
		android:layout_weight="1" android:orientation="horizontal">
		<include layout="@layout/nav_bar" android:id="@+id/linearLayout2_ref"
			android:layout_height="match_parent" android:layout_width="wrap_content" />
		<LinearLayout android:id="@+id/linearLayout3"
			android:orientation="vertical" android:layout_width="fill_parent"
			android:layout_height="fill_parent">
			<TextView android:layout_height="wrap_content"
				android:textStyle="bold" android:layout_width="match_parent" android:text="@string/txt_edit_log_entry" android:textSize="20dip" android:id="@+id/txt_title"></TextView>
			<TextView android:text="TextView" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/txt_timestamp"></TextView>
			<EditText android:layout_width="match_parent" android:text="EditText" android:layout_height="wrap_content" android:id="@+id/ed_starlog_text"></EditText>
			<Button android:layout_width="wrap_content" android:text="@string/txt_btn_confirm" android:id="@+id/button1" android:layout_height="wrap_content" android:layout_gravity="right" android:onClick="onConfirmButtonClick"></Button>
			<View android:id="@+id/view3" android:layout_height="16dip"
				android:layout_width="wrap_content" style="@style/PanelElement"
				android:background="@color/orange"></View>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
